+		reg = <0x0 0xff210000 0x0 0x20>;
+	};
+
+	qos_debug: qos@ff210080 {
+		compatible = "syscon";
+		reg = <0x0 0xff210080 0x0 0x20>;
+	};
+
+	qos_gpu_m0: qos@ff220000 {
