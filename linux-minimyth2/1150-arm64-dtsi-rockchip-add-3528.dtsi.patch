diff --speed-large-files --no-dereference --minimal -Naur linux-6.4.11/arch/arm64/boot/dts/rockchip/rk3528.dtsi linux-6.4.11/arch/arm64/boot/dts/rockchip/rk3528.dtsi
--- linux-6.4.11/arch/arm64/boot/dts/rockchip/rk3528.dtsi	1970-01-01 01:00:00.000000000 +0100
+++ linux-6.4.11/arch/arm64/boot/dts/rockchip/rk3528.dtsi	2023-08-24 15:31:42.879895505 +0200
@@ -0,0 +1,1802 @@
+// SPDX-License-Identifier: (GPL-2.0+ OR MIT)
+/*
